import math

a = float(input("a = "))
b = float(input("b = "))
c = float(input("c = "))
D = math.sqrt(b ** 2) - (4 * a * c)
if D == 0:
    x1 = (-b) / 2 * a
elif D > 0:
    x1 = (-b - D) / 2 * a
    x2 = (-b + D) / 2 * a
else:
    print("нет действительных корней")

print(x1, x2, D)
